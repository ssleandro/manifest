<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote fetch="git://github.com" name="github"/>
  <remote fetch="git://git.openembedded.org" name="oe"/>
  <remote fetch="ssh://code.ossystems.com.br" name="ossystems" review="code.ossystems.com.br"/>
  <remote fetch="git://git.yoctoproject.org" name="yocto"/>

  <default remote="ossystems" revision="InvalidRevision" sync-j="4"/>

  <!-- OpenEmbedded Layers -->
  <project name="bitbake" path="sources/bitbake" remote="oe" revision="1.44"/>
  <project name="openembedded-core" path="sources/openembedded-core" revision="zeus"/>
  <project name="meta-openembedded" path="sources/meta-openembedded" remote="oe" revision="zeus"/>

  <!-- OS Systems Layers -->
  <project name="meta-ossystems-base" path="sources/meta-ossystems-base" revision="zeus"/>
  <project name="ossystems-yocto-base-scripts" path="sources/base" revision="zeus">
    <copyfile dest="setup-environment" src="setup-environment"/>
  </project>

  <project name="apps/ye" path="sources/ye" revision="master"/>

  <!-- UpdateHub Layer -->
  <project remote="github" revision="zeus" name="UpdateHub/meta-updatehub" path="sources/meta-updatehub"/>

  <!-- ShellHub layer -->
  <project remote="github" revision="zeus" name="shellhub-io/meta-shellhub" path="sources/meta-shellhub"/>

  <!-- Inobram Layer -->
  <project name="inobram/smaai5/meta-smaai5" path="sources/meta-smaai5" revision="6.1.x"/>
  <project name="inobram/smaai5/yocto-config" path="sources/conf/tools" revision="6.1.x"/>
  <project name="inobram/smaai5/yocto-platform" path="sources/conf/repo" revision="6.1.x"/>
</manifest>
